	fileso=`echo "$$files" | sed 's|$$|o|'`; \
	echo " ( cd '$(DESTDIR)$(atsc_pythondir)' && rm -f" $$files ")"; \
	cd "$(DESTDIR)$(atsc_pythondir)" && rm -f $$files || exit $$?; \
	echo " ( cd '$(DESTDIR)$(atsc_pythondir)' && rm -f" $$filesc ")"; \
	cd "$(DESTDIR)$(atsc_pythondir)" && rm -f $$filesc || exit $$?; \
