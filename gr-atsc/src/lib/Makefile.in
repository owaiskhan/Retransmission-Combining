install-atsc_pythonPYTHON: $(atsc_python_PYTHON)
	@$(NORMAL_INSTALL)
	test -z "$(atsc_pythondir)" || $(MKDIR_P) "$(DESTDIR)$(atsc_pythondir)"
	@list='$(atsc_python_PYTHON)'; dlist=; list2=; test -n "$(atsc_pythondir)" || list=; \
	for p in $$list; do \
	  if test -f "$$p"; then b=; else b="$(srcdir)/"; fi; \
	  if test -f $$b$$p; then \
	    $(am__strip_dir) \
	    dlist="$$dlist $$f"; \
	    list2="$$list2 $$b$$p"; \
	  else :; fi; \
	done; \
	for file in $$list2; do echo $$file; done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(atsc_pythondir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(atsc_pythondir)" || exit $$?; \
	done || exit $$?; \
	if test -n "$$dlist"; then \
	  if test -z "$(DESTDIR)"; then \
	    PYTHON=$(PYTHON) $(py_compile) --basedir "$(atsc_pythondir)" $$dlist; \
	  else \
	    PYTHON=$(PYTHON) $(py_compile) --destdir "$(DESTDIR)" --basedir "$(atsc_pythondir)" $$dlist; \
	  fi; \
	else :; fi

